public class Day4_RelationalOperator {
    public static void main(String[] args) {
        int a = 96;
        int b = 87;

        // Check Equal To
        System.out.println(a==b);

        // Check Not Equal TO
        System.out.println(a!=b);

        // Greater than
        System.out.println(a>b);

        // Less Than
        System.out.println(a<b);

        // Greater than Equal To
        System.out.println(a>=b);

        // Less Than Equal To
        System.out.println(a<=b);
    }
}
